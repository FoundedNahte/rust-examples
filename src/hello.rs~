    println!("{number:0>width$}", number=1, width=6);

    println!("My name is {0}, {1} {0}", "bond", "James");

    println!("Pi is roughly {number})

    #[allow(dead_code)]
    struct Structure(i32);

    //println!("This struct '{}' won't print...", Structure(3))};
